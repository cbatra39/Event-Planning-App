<!DOCTYPE html>
<html>
  <head>
    <title>EventPlanning- AdminPanel</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
        <!-- Favicons -->
    <link href="<%= asset_path('favicon.png') %>" rel="icon">
    <link href="<%= asset_path('apple-touch-icon.png') %>" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <!-- Vendor CSS Files -->
    <link href="<%= asset_path('vendor/bootstrap/css/bootstrap.min.css') %>" rel="stylesheet">
    <link href="<%= asset_path('vendor/bootstrap-icons/bootstrap-icons.css') %>" rel="stylesheet">
    <link href="<%= asset_path('vendor/boxicons/css/boxicons.min.css') %>" rel="stylesheet">
    <link href="<%= asset_path('vendor/quill/quill.snow.css') %>" rel="stylesheet">
    <link href="<%= asset_path('vendor/quill/quill.bubble.css') %>" rel="stylesheet">
    <link href="<%= asset_path('vendor/remixicon/remixicon.css') %>" rel="stylesheet">
    <link href="<%= asset_path('vendor/simple-datatables/style.css') %>" rel="stylesheet">

    <!-- Template Main CSS File -->
	  <%= stylesheet_link_tag "style.css" %>
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
   <script src="https://kit.fontawesome.com/2f2502f7e5.js" crossorigin="anonymous"></script>
   <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
   <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script> 
  <script src="https://kit.fontawesome.com/2f2502f7e5.js" crossorigin="anonymous"></script>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
  
    <% if notice %>
      <div class="alert alert-warning alert-dismissible fade show" role="alert">
      <%= notice %>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    <%end%>
   


  
    <%= yield %>
    
     <!-- Vendor JS Files -->
    <script src="<%= asset_path('vendor/apexcharts/apexcharts.min.js') %>"></script>
    <script src="<%= asset_path('vendor/bootstrap/js/bootstrap.bundle.min.js') %>"></script>
    <script src="<%= asset_path('vendor/chart.js/chart.umd.js') %>"></script>
    <script src="<%= asset_path('vendor/echarts/echarts.min.js') %>"></script>
    <script src="<%= asset_path('vendor/quill/quill.min.js') %>"></script>
    <script src="<%= asset_path('vendor/simple-datatables/simple-datatables.js') %>"></script>
    <script src="<%= asset_path('vendor/tinymce/tinymce.min.js') %>"></script>
    <script src="<%= asset_path('vendor/php-email-form/validate.js') %>"></script>

  <!-- Template Main JS File -->
    <script src="<%= asset_path('main.js') %>"></script>
  </body>
</html>
